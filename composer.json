{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "acaronlex/laravel-calendar": "^1.3",
        "akaunting/laravel-money": "^1.2",
        "appointer/swaggervel": "dev-master",
        "arielmejiadev/larapex-charts": "dev-master",
        "arrilot/laravel-widgets": "^3.13",
        "barryvdh/laravel-snappy": "^0.4.8",
        "cache/filesystem-adapter": "^1.1",
        "digitalcloud/laravel-blameable": "^1.1",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "genealabs/laravel-model-caching": "^0.11.3",
        "guzzlehttp/guzzle": "^7.0.1",
        "infyomlabs/coreui-templates": "8.0.x-dev",
        "infyomlabs/generator-builder": "dev-master",
        "infyomlabs/laravel-generator": "dev-develop",
        "infyomlabs/swagger-generator": "dev-master",
        "jenssegers/date": "^4.0",
        "kalnoy/nestedset": "^6.0",
        "laravel/framework": "^8.12",
        "laravel/tinker": "^2.5",
        "laravel/ui": "3.0",
        "laravelcollective/html": "^6.2",
        "ramsey/uuid-doctrine": "^1.6",
        "spatie/laravel-menu": "^3.7",
        "spatie/laravel-mix-preload": "^1.2",
        "spatie/laravel-permission": "^4.0",
        "spatie/menu": "^2.10",
        "swayok/alternative-laravel-cache": "^6.1",
        "watson/rememberable": "^5.0",
        "yajra/laravel-datatables": "^1.5"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.6",
        "bennett-treptow/laravel-migration-generator": "^3.2",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/dusk": "^6.16",
        "laravel/sail": "^0.0.5",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "repositories": [
        {
            "type": "vcs",
            "url":  "git@github.com:InfyOmLabs/swaggervel.git"
        },        
        {
            "type": "vcs",
            "url": "git@github.com:pandigresik/larapex-charts.git"            
        }

    ],
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": ["jamesmills/laravel-timezone"]
        }
    },
    
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "InfyOm\\Generator\\Generators\\": "overrides/laravel-generator/src/Generators/",
            "InfyOm\\Generator\\Commands\\": "overrides/laravel-generator/src/Commands/"
        },
        "exclude-from-classmap": [
            "vendor/infyomlabs/laravel-generator/src/Generators/Scaffold/ControllerGenerator.php",
            "vendor/infyomlabs/laravel-generator/src/Generators/Scaffold/ViewGenerator.php",
            "vendor/infyomlabs/laravel-generator/src/Generators/FactoryGenerator.php",
            "vendor/infyomlabs/laravel-generator/src/Commands/BaseCommand.php"
        ],
        "files": [
            "app/Http/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
